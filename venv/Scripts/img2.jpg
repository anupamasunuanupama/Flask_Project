            function BlogLoadMore(elem) {
                this.$elem      = $(elem);
                this.$triggers  = this.$elem.find('[data-blogloadmore-trigger]');

                this.$triggers.each( this.bindUIActions(this) );
            };
          
            BlogLoadMore.prototype = {
                
                constructor : BlogLoadMore,
                
                bindUIActions: function(self) {
                    return function(index, element) {
