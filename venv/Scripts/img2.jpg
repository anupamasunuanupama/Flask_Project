         */

        ;(function($) {
          
            function BlogAffix(elem) {
                this.$elem      = $(elem);

                this.$elem.wrap( '<div class="affix-container"></div>' );
                this.$elem.each( this.bindUIActions(this) );
            };
          
            BlogAffix.prototype = {
                
                constructor : BlogAffix,
                
                bindUIActions: function(self) {
                    return function(index, element) {
                        var targetPos = $(element).offset().top;
